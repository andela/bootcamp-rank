.fixed-action-btn.hide-on-med-and-up(style='bottom: 45px; right: 24px;' ng-if="isLoggedIn")
  a.btn-floating.btn-large.red
    i.large.material-icons mode_edit
  ul
    li
      a.btn-floating.red
        i.material-icons insert_chart
    li
      a.btn-floating.yellow.darken-1
        i.material-icons format_quote
    li
      a.btn-floating.green
        i.material-icons publish
    li
      a.btn-floating.blue
        i.material-icons attach_file


footer.page-footer.grey.darken-3(ng-controller="FooterCtrl")
  .container
    .row
      .col.l6.s12
        h5.white-text {{footer.appName}}
        p.grey-text.text-lighten-4 {{footer.description}}
      .col.l4.offset-l2.s12
        h5.white-text Social Networks
        ul
          li
            a.grey-text.text-lighten-3(href='#!')
              i.fa.fa-facebook-square.fa-1x
              |   Facebook
          li
            a.grey-text.text-lighten-3(href='#!')
              i.fa.fa-google-plus-square.fa-1x
              |   Google
          li
            a.grey-text.text-lighten-3(href='#!')
              i.fa.fa-linkedin-square.fa-1x
              |   Linkedin
          li
            a.grey-text.text-lighten-3(href='#!')
              i.fa.fa-youtube-square.fa-1x
              |   Youtube
  .footer-copyright
    .container
      | Â© 2014 Copyright Text
      a.grey-text.text-lighten-4.right(href='#!') More Links

script(src="lib/jquery/dist/jquery.min.js")
script(src='https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.1/js/materialize.min.js')
script(src="lib/Materialize/dist/js/materialize.min.js")
